{% extends "layout.twig" %}
{% block title %}
    Create new issue
{% endblock %}
{% block content %}
<form method="post" action="" enctype="application/x-www-form-urlencoded">
    <div class="form-group">
        <label for="issue-name">Name</label>
        <input type="text" class="form-control" name="issue-name" id="issue-name" placeholder="Enter issue name">
    </div>
    <div class="form-group">
        <label for="user">User</label>
        <input type="text" class="form-control" name="user" id="user" placeholder="Enter user name">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
{% endblock %}
